<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>

	<typeAliases>
		<typeAlias alias="methodLog" type="kr.pe.felixproject.common.service.MethodLog" />
		<typeAlias alias="webLog" type="kr.pe.felixproject.common.service.WebLog" />
		<typeAlias alias="program" type="kr.pe.felixproject.common.service.Program" />

		<typeAlias alias="webLogVO" type="kr.pe.felixproject.common.service.WebLogVO" />
		<typeAlias alias="programVO" type="kr.pe.felixproject.common.service.ProgramVO" />
	</typeAliases>
	
	<plugins>
		<plugin interceptor="kr.pe.felixproject.core.MyBatisLogInterceptor" />
	</plugins>

</configuration>
